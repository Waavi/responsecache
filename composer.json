{
    "name": "waavi/responsecache",
    "description": "Response cache for Laravel 5.1",
    "keywords": [
        "waavi",
        "laravel",
        "responsecache",
        "cache",
        "response cache"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Waavi",
            "email": "info@waavi.com",
            "homepage": "http://waavi.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.1",
        "illuminate/cache": "^5.1",
        "illuminate/http": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4",
        "orchestra/testbench": "~3.1",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Waavi\\ResponseCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waavi\\ResponseCache\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}